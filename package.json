{
  "name": "redocx",
  "description": "Create word documents with React",
  "version": "1.1.4",
  "repository": "https://github.com/nitin42/redocx",
  "main": "./lib/index.js",
  "author": "Nitin Tulswani",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "NODE_ENV=production babel src -d lib",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "example": "babel-node ./demo/word.js",
    "example-mem": "babel-node ./demo/word-memory.js",
    "lint": "eslint ./src",
    "prebuild": "rm -rf ./lib"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --no-semi --write",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "chroma-js": "^1.3.4",
    "execa": "^0.8.0",
    "fbjs": "^0.8.4",
    "memory-streams": "0.1.3",
    "minimatch": "^3.0.4",
    "officegen": "^0.6.4",
    "react": "^16.0.0",
    "react-reconciler": "0.2.0",
    "validator": "^8.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "7",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "diff-react": "^1.0.5",
    "eslint": "^4.4.1",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "jest": "^20.0.4",
    "lint-staged": "^4.2.1",
    "prettier": "^1.7.0"
  },
  "keywords": [
    "renderer",
    "react",
    "word documents",
    "custom renderer",
    "fiber"
  ]
}
